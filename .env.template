# Environment Configuration Template
# Copy to .env and fill in your values

# Kaggle API Credentials
KAGGLE_USERNAME=your_kaggle_username
KAGGLE_KEY=your_kaggle_api_key

# Model Configuration
MODEL_NAME=Open-Orca/orca_mini_3b
DEVICE=auto
TORCH_DTYPE=float16

# LLM Reasoning Parameters
MAX_TOKENS=512
TEMPERATURE=0.7
TOP_P=0.9

# Runtime Configuration
AIMO_ENV=development
LOG_LEVEL=INFO
DEBUG=False

# GPU Configuration
CUDA_VISIBLE_DEVICES=0
WANDB_DISABLED=True

# Kaggle Submission
SUBMISSION_NAME=AIMO3_Submission
SUBMISSION_MESSAGE=Updated submission with improved reasoning

# Paths (optional - defaults in config.py)
# OUTPUT_DIR=outputs
# LOGS_DIR=logs
# DATA_DIR=datasets
